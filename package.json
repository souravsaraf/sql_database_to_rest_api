{
  "name": "sql_database_to_rest_api",
  "version": "1.0.0",
  "description": "sql_database_to_rest_api",
  "main": "dist/app.js",
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src"
    ],
    "ext": "js ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "exec": "tsc && node dist/app.js"
  },
  "scripts": {
    "start": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rest_api",
    "sql",
    "database",
    "typescript"
  ],
  "author": "souravsaraf",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "nodemon": "^1.19.2",
    "tslint": "^5.19.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.140",
    "@types/node": "^12.7.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "mssql": "^5.1.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "pg": "^7.12.1",
    "sqlite3": "^4.1.0"
  }
}
